{
  "name": "Website",
  "dockerComposeFile": "docker-compose.yml",
  "service": "website",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": [
        "dbaeumer.vscode-eslint",
        "csstools.postcss",
        "esbenp.prettier-vscode",
        "bradlc.vscode-tailwindcss"
      ]
    }
  },
  "forwardPorts": [3000],
  "postCreateCommand": "npm i",
  "remoteUser": "me",
  "features": {
    "git": "latest",
    "git-lfs": "latest"
  }
}
